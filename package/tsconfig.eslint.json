{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "src/*": [
        "./*"
      ],
      "src": [
        "."
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "**/*.mjs",
    "**/*.mjsx",
    "**/*.cjs",
    "**/*.cjsx",
    "**/.*.ts",
    "**/.*.tsx",
    "**/.*.js",
    "**/.*.jsx",
    "**/.*.mjs",
    "**/.*.mjsx",
    "**/.*.cjs",
    "**/.*.cjsx"
  ],
  "exclude": [
    "node_modules"
  ]
}