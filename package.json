{
  "name": "@pija-ab/next-dashboard",
  "version": "7.0.1",
  "description": "> TODO: description",
  "author": "Linn Dahlgren <linn.dahlgren@pija.se>",
  "homepage": "https://github.com/PiJaAB/next-dashboard#readme",
  "license": "UNLICENSED",
  "main": "dist/entry.js",
  "directories": {
    "assets": "assets",
    "dist": "dist",
    "src": "src",
    "styles": "styles",
    "test": "__tests__"
  },
  "files": [
    "assets",
    "dist",
    "styles",
    "src",
    "tailwind.config.js",
    "postcss.config.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PiJaAB/next-dashboard.git"
  },
  "scripts": {
    "dev": "tsc --watch",
    "dev:pkg": "nodemon -w src -x \"yarn run -s build\"",
    "lint": "run-p -l --silent --aggregate-output --continue-on-error lint:*",
    "lint:eslint-check": "NODE_ENV=production eslint-config-prettier 'src/**/*.js'",
    "lint:prettier": "NODE_ENV=production prettier -c 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:eslint": "NODE_ENV=production eslint src --report-unused-disable-directives --color",
    "lint:stylelint": "NODE_ENV=production stylelint styles/**/*.css --rd --risd --color",
    "build": "run-s -l --silent build:clean build:tsc",
    "build:clean": "rimraf dist",
    "build:tsc": "tsc",
    "prepack": "yarn build",
    "test": "yarn run --silent lint",
    "deploy": "node scripts/deploy.js"
  },
  "nodemonConfig": {
    "ext": "js,jsx,ts,tsx,json,css,scss,svg",
    "watch": [
      "src"
    ]
  },
  "bugs": {
    "url": "https://github.com/PiJaAB/next-dashboard/issues"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
